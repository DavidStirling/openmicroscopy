<FindBugsFilter>

  <!--
    The code generated by slice2java is out of our control, and
    should be filtered from bug detection.
  -->
  <Match> <Package name="omero.api"/> </Match>
  <Match> <Package name="omero.grid"/> </Match>
  <Match> <Package name="omero.model"/> </Match>
  <Match> <Package name="omero.sys"/> </Match>
  
  <!--
    Filter out any Unread Field Bug if the field in the class is called log
  -->
  <Match>
    <Bug pattern="URF_UNREAD_FIELD" />
    <Field name="log" />
  </Match>
</FindBugsFilter>


